{
    "multipart": [
        {   "apply": { "model": "dawnoftimebuilder:block/french/oak_support_slab" }
        },
        {   "when": { "axis": "x", "subaxis": "false"},
            "apply": { "model": "dawnoftimebuilder:block/french/oak_beam_x" }
        },
        {   "when": { "axis": "z", "subaxis": "false"},
            "apply": { "model": "dawnoftimebuilder:block/french/oak_beam_x", "y": 90 }
        },
        {   "when": { "subaxis": "true" },
            "apply": { "model": "dawnoftimebuilder:block/french/oak_beam_x_z" }
        },
        {   "when": { "pillar_connection": "4_pixels" },
            "apply": { "model": "dawnoftimebuilder:block/french/oak_support_4_pixels" }
        }
    ]
}