{
	"multipart": [
		{
			"when": { "axis": "y" },
			"apply": { "model": "dawnoftimebuilder:block/french/oak_beam_y"}
		},
		{
			"when": {"OR": [
				{"axis": "x", "subaxis_z": "false" },
				{"axis": "y", "subaxis_x": "true", "subaxis_z": "false" }
			]},
			"apply": { "model": "dawnoftimebuilder:block/french/oak_beam_x"}
		},
		{
			"when": {"OR": [
				{"axis": "z", "subaxis_x": "false" },
				{"axis": "y", "subaxis_z": "true", "subaxis_x": "false" }
			]},
			"apply": { "model": "dawnoftimebuilder:block/french/oak_beam_x", "y": 90}
		},
		{
			"when": {"OR": [
				{"axis": "x", "subaxis_z": "true" },
				{"axis": "z", "subaxis_x": "true" },
				{"subaxis_z": "true", "subaxis_x": "true" }
			]},
			"apply": { "model": "dawnoftimebuilder:block/french/oak_beam_x_z"}
		},
		{
			"when": { "bottom": "true" },
			"apply": { "model": "dawnoftimebuilder:block/french/oak_beam_bottom"}
		}
	]
}